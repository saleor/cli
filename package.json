{
  "name": "saleor-cli",
  "version": "1.12.0-rc.2",
  "description": "",
  "type": "module",
  "repository": "github:saleor/saleor-cli",
  "homepage": "https://github.com/saleor/saleor-cli#readme",
  "bugs": "https://github.com/saleor/saleor-cli/issues",
  "bin": {
    "saleor": "./build/cli.js"
  },
  "scripts": {
    "prepublishOnly": "tsc",
    "compile": "tsc",
    "watch": "tsc -w",
    "package": "pkg .",
    "lint": "prettier --write . && eslint src/**/*.ts --cache --fix",
    "generate": "graphql-codegen",
    "postinstall": "node ./scripts/binary.js install",
    "test": "pnpm uvu -r tsm spec",
    "prepare": "husky install"
  },
  "files": [
    "build",
    "template",
    "scripts"
  ],
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node16-linux",
      "node16-macos",
      "node16-win"
    ],
    "outputPath": "release"
  },
  "author": "Saleor",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/typescript-document-nodes": "^2.3.2",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/git-url-parse": "^9.0.1",
    "@types/node": "^18.6.3",
    "@types/prompt": "^1.1.2",
    "@types/tar": "^6.1.2",
    "@types/update-notifier": "^6.0.1",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "clean-publish": "^4.0.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "pkg": "^5.8.0",
    "prettier": "2.7.1",
    "tsm": "^2.2.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.13.1",
    "@oclif/core": "^1.13.1",
    "@sentry/node": "^7.8.1",
    "@types/detect-port": "^1.3.2",
    "@types/lodash.kebabcase": "^4.1.7",
    "aws-amplify": "^4.3.30",
    "boxen": "^7.0.0",
    "chalk": "^5.0.1",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.11.2",
    "date-fns": "^2.29.1",
    "debug": "^4.3.4",
    "detect-port": "^1.3.0",
    "dotenv": "^16.0.1",
    "emphasize": "^5.0.0",
    "enquirer": "^2.3.6",
    "fetch-repo-dir": "^1.0.6",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "git-url-parse": "^12.0.0",
    "got": "^12.3.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "lodash.kebabcase": "^4.1.1",
    "lookpath": "^1.2.2",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.2.10",
    "ora": "^6.1.2",
    "prompt": "^1.3.0",
    "replace-in-file": "^6.3.5",
    "retes": "^0.31.1",
    "sanitize-filename": "^1.6.3",
    "simple-git": "^3.11.0",
    "slugify": "^1.6.5",
    "tar": "^6.1.11",
    "tplv": "^1.0.0",
    "update-notifier": "^6.0.2",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "overrides": {
      "uuid": "8.3.2"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,json}": "prettier --write"
  }
}
